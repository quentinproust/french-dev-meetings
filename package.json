  "private": true,
  "dependencies": {
    "leaflet": "^1.2.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-leaflet": "^1.7.8",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "npm run meetings && react-scripts start",
